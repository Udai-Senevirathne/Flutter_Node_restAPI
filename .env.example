# For the Developers


# Environment Configuration for MySQL REST API
# Copy this file to .env and update with your database credentials

# Application Settings
NODE_ENV=development
PORT=3000

# MySQL Database Configuration
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=
MYSQL_DATABASE=rest_api_db

# Security (for future features)
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRE=7d
